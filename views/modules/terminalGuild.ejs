<style>
  .guild-information {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    grid-template-rows: repeat(2, 1fr);
    gap: 8px;
    height: 224px;
    width: 100%;
  }
  .inform-panel {
    width: calc(100% - 24px);
    height: calc(100% - 24px);
    background: var(--c-36);
    border-radius: 17px;
    padding: 12px;
    gap: 8px;
  }
  .main-inform {
    width: calc(100% - 12px);
    height: 38px;
    padding: 6px;
  }
  .main-inform > img {
    height: 38px;
    pointer-events: none;
    border-radius: 11px;
  }
  .delete-block .top {
    height: 38px;
    width: 100%;
  }
  .delete-block .top > p {
    padding-left: 8px;
    font-size: 18px;
    text-shadow: 0px 3px 8px var(--c-24);
    font-weight: 600;
    height: 38px;
    flex: 1;
  }
  .delete-block .bottom {
    gap: 8px;
  }
  .delete-block .bottom > button {
    display: flex;
    flex-direction: row;
    width: 100%;
    overflow: hidden;
  }
  .delete-block .bottom > button img {
    padding: 6px;
    height: 35px;
  }
  .delete-block .bottom > button > p {
    font-size: 12px;
    width: min-content;
    flex: 1;
    height: 100%;
    padding-left: 8px;
    text-align: left;
    overflow: hidden;
  }
  .delete-block .icon-wrapper {
    height: 44px;
    width: 44px;
    border-radius: 5px;
  }
  .delete-block .trash {
    background: #493333;
  }
  .delete-block .shield {
    background: #354933;
  }
  .delete-block .discord {
    background: #333849;
  }
</style>
<div class="flex align-center main-inform default-alpha-block">
  <img src="<%= guild.avatar %>">
</div>
<div class="guild-information">
  <div class="default-alpha-block flex inform-panel" style="grid-column: 1 / 2; grid-row: 1 / 2;">
  </div>
  <%- include('C:\\Users\\Марк\\[КОД]\\Web\\Guild Menegment\\views\\modules\\terminal\\guild\\8-ght-panel.ejs', guild) %>
</div>