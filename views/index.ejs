</head>
<% const guildParams = [
  { name: "Fax", 
    logo: "https://cdn.discordapp.com/icons/806816080508354611/4dd75efaafb2cc17185720ce85f0a87c.webp?size=240",
    desc: "#1 S21 | More: @faxnocap"
  }
]; %>
<body>
<div class="preloader none center">
  <div class="honeycomb">
    <div></div>
    <div></div>
    <div></div>
    <div></div>
    <div></div>
    <div></div>
    <div></div>
  </div>
</div>
<background class="background-global absolute flex">
  <div class="rocks rocks2 absolute">
    <img class="w-ful" src="https://api.impactium.fun/svg/1-01.svg">
  </div>
  <div class="rocks absolute">
    <img class="w-ful" src="https://api.impactium.fun/svg/2-01.svg">
  </div>
  <div class="rocks starsX absolute">
    <img src="https://api.impactium.fun/ui/diamond-full-pattern.svg">
  </div>
  <div class="rocks rocks5 absolute">
    <img src="https://api.impactium.fun/svg/rocks5.svg">
  </div>
  <div class="rocks absolute">
    <img class="w-ful" src="https://api.impactium.fun/svg/7-03.svg">
  </div>
</background>
<langpanel>
    <p>Lang: <img class="main-lang" src="https://api.impactium.fun/langs/<%= lang.mainLang %>.png"></p>
    <div>
      <% const availableLanguages = ['en', 'uk', 'ru', 'it', 'is']; %>
      <% for (const langCode of availableLanguages) { %>
        <% if (langCode !== lang.mainLang) { %>
          <a href="/lang/<%= langCode %>"><img src="https://api.impactium.fun/langs/<%= langCode %>.png"></a>
        <% } %>
      <% } %>
    </div>
</langpanel>
<header class="flex flex-dir-row center-v">
  <h1>Impactium</h1>
  <div class="buttons flex">
    <button class="flex-dir-row center" id="partners">
      <%= lang.partners %>
      <img class="aNb" src="https://api.impactium.fun/ux/to-left.svg">
      <navBar>
        <% guildParams.forEach(guild => { %>
        <div class="partners-unique-guild flex-dir-row center-v" onclick="redirect('<%= guild.name %>')">
          <div class="guild-avatar flex center">
            <img src="<%= guild.logo %>">
          </div>
          <div class="partners-unique-guild-text flex-dir-column">
            <h3><%= guild.name %></h3>
            <div class="p-wrapper">
              <p><%= guild.desc %></p>
            </div>
          </div>
        </div>
        <% }); %>
      </navBar>
    </button>
    <line></line>
    <button class="flex-dir-row center" id="products">
      <%= lang.products %>
      <img class="aNb" src="https://api.impactium.fun/ux/to-left.svg">
      <navBar>
        <div class="partners-unique-guild flex-dir-row center-v" onclick="redirectDiscordBotJoin()">
          <div class="guild-avatar flex center">
            <img src="https://api.impactium.fun/logo/impactium-wb.png">
